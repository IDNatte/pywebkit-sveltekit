{
	"name": "pywebkit-sveltekit",
	"version": "0.0.1",
	"scripts": {
		"app:dev": "APP_DEV=\"True\" concurrently --names \"Sveltekit,Python\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn run svelte:dev\" \"yarn run python:dev\"",
		"svelte:dev": "vite dev",
		"svelte:build": "vite build",
		"svelte:preview": "vite preview",
		"svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"svelte:check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"svelte:test:unit": "vitest",
		"svelte:lint": "prettier --plugin-search-dir . --check . && eslint .",
		"svelte:format": "prettier --plugin-search-dir . --write .",
		"python:dev": "./.venv/Scripts/python ./window/app.py",
		"python:build": "yarn run svelte:build && ./.venv/Scripts/pyinstaller Nyanga-Read.spec"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@ptkdev/sveltekit-electron-adapter": "^0.3.0",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.20.2",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.9",
		"concurrently": "^8.0.1",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.2.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.4",
		"vite": "^4.2.3",
		"vitest": "^0.30.1"
	},
	"type": "module"
}
